# Caddyfile
https://yaarfetch-production.up.railway.app {
    handle {
        reverse_proxy localhost:3000 {
            header_up Access-Control-Allow-Origin "https://serene-embrace-production.up.railway.app"
            header_up Access-Control-Allow-Methods "GET, POST, PUT, DELETE, OPTIONS"
            header_up Access-Control-Allow-Headers "Content-Type, Authorization"
            header_up Access-Control-Allow-Credentials "true"
        }
    }
}
